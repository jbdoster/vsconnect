{
  "name": "vsconnect",
  "displayName": "vsconnect",
  "description": "Connect your VS Code commands to your USB device today!",
  "version": "1.0.0-alpha",
  "publisher": "ASTROLABZ",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbdoster/vsconnect"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.CONNECT",
        "title": "VSConnect"
      },
      {
        "command": "extension.REMOVE_CONNECTED_VSCODE_COMMAND",
        "title": "VSConnect: Remove VS Command Connection"
      }

    ]
  },
  "scripts": {
    "circular-dependency-check": "madge --circular --extensions ts ./src",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "electron-rebuild": "^3.2.7",
    "eslint": "^7.21.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "madge": "^5.0.1",
    "usb": "^2.1.2"
  }
}
