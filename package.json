{
  "name": "vsconnect",
  "displayName": "vsconnect",
  "description": "Connect your VS Code commands to your USB device today!",
  "version": "1.0.0-alpha",
  "publisher": "ASTROLABZ",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbdoster/vsconnect"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.CONNECT",
        "title": "VSConnect"
      },
      {
        "command": "extension.REMOVE_CONNECTED_VSCODE_COMMAND",
        "title": "VSConnect: Remove VS Command Connection"
      }
    ]
  },
  "scripts": {
    "circular-dependency-test": "madge --circular --extensions ts ./src",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "unit:test": "jest --config ./src/jest.config.js --coverage --verbose",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.2",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "electron-rebuild": "^3.2.7",
    "eslint": "^7.21.0",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "madge": "^5.0.1",
    "ts-node": "^10.9.1",
    "usb": "^2.1.2",
    "vsce": "^2.14.0"
  }
}
